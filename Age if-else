//# age
//c++ program to determine whether you are eligible to vote or not
#include<iostream>
#include <stdio.h>
using namespace std;

int main()
{
    int age=0;
    std ::cout<<"Enter your the age "<<endl;
    std ::cin>>age;
    if(age>=18)
        std ::cout<<"You are eligible to vote";
    else
        std ::cout<<"You are not eligible to vote";
    return 0;
}
